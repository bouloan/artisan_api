    "version": "0.2.0",
    "configurations": [{
        "type": "node",
        "request": "launch",
        "name": "Artisan api Program",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "sourceMaps": true,
        "smartStep": true,
        "preLaunchTask": "npm: dev",
        "program": "${workspaceFolder}//src//app.ts",
        "outFiles": [
            "${workspaceFolder}//dist//app.js"
        ]
    }]
}